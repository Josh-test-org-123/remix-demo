{
  "name": "remix-demo",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build && wrangler pages functions build --outdir=./dist/worker/ && cp .assetsignore ./build/client/",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler dev ./dist/worker/index.js --assets ./build/client/",
    "typecheck": "tsc",
    "preview": "wrangler dev ./dist/worker/index.js --assets ./build/client/",
    "deploy": "npm run build && wrangler deploy ./dist/worker/index.js"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^2.4.1",
    "@remix-run/cloudflare-pages": "^2.4.1",
    "@remix-run/react": "^2.4.1",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "@remix-run/dev": "^2.4.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript": "^5.1.6",
    "vite": "^5.0.0",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "^3.15.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": ""
}
